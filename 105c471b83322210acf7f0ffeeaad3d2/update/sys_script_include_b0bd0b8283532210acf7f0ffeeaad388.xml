        <script><![CDATA[   var DistributionListUtils = Class.create();
   DistributionListUtils.prototype = {
       initialize: function() {},
       
       getDistributionList: function(id) {
           var gr = new GlideRecord('x_1645407_distlist_dln');
           if (gr.get(id)) {
               return {
                   name: gr.name,
                   displayName: gr.display_name_of_the_distribution_list_in_outlook,
                   owners: gr.distribution_list_owners,
                   members: gr.distribution_list_members
               };
           }
           return null; // Return null if not found
       },
       
       type: 'DistributionListUtils'
   };
]]></script>
